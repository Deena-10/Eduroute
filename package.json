{
  "name": "career-roadmap-app",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "firebase": "^12.1.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "concurrently \"npm run backend\" \"npm run frontend\"",
    "frontend": "cd frontend && npm start",
    "backend": "cd backend && npm start",
    "ai-service": "cd backend/service && python application.py",
    "build": "cd frontend && npm run build",
    "build:production": "npm run build && npm run build:backend",
    "build:backend": "cd backend && npm install --production",
    "test": "cd frontend && npm test",
    "test-integration": "node test-integration.js",
    "eject": "cd frontend && npm run eject",
    "deploy:build": "build-production.bat"
  },
  "devDependencies": {
    "concurrently": "^9.2.0"
  }
}
